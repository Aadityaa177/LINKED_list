#include <iostream>
using namespace std;

struct node
{
    int data;
    struct node* nxt;
}*temp, *newnode, *head, *tail;

void insert(int element)
{
    newnode = (struct node*)malloc(sizeof(struct node));
    newnode->data = element;
    newnode->nxt = NULL;
    if(head == NULL)
    {
        head = newnode;
        tail = newnode;
        return;
    }
    else if(head->nxt==NULL)
    {
        head->nxt = newnode;
        tail = newnode;
    }
    else
    {
        newnode->nxt = head;
        head = newnode;
    }
}

void del()
{
    if(head==NULL)
    {
        cout<<"List is empty"<<endl;
    }
    else if(head==tail)
    {
        cout<<"only one element is present"<<endl;
        delete tail;
        tail = NULL;
        head = NULL;
    }
    else
    {
        temp = head;
        while(temp->nxt!=tail)
        {
            temp = temp->nxt;
        }
        delete tail;
        tail = temp;
        tail->nxt = NULL;
    }
}

void display()
{
    if(head == NULL)
    {
        cout<<"List is empty"<<endl;
    }
    else
    {
        cout<<"List of elements are:"<<endl;
        for(temp = head; temp != NULL; temp = temp->nxt)
        {
            cout<<temp->data<<" ";
        }
        cout<<endl;
    }
}

int main()
{
    insert(9);
    insert(5);
    insert(14);
    insert(7);
    insert(5);
    display();
    del();
    display();
    del();
    display();
    del();
    display();
    del();
    display();
    del();
    display();
}


"""output:
List of elements are:
5 7 14 9 5 
List of elements are:
5 7 14 9 
List of elements are:
5 7 14 
List of elements are:
5 7 
List of elements are:
5 
only one element is present
List is empty
"""
